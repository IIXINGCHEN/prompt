{
    "mcpServers": {
      "Sequential thinking": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-sequential-thinking"
        ]
      },
      "Context 7": {
        "command": "npx",
        "args": [
          "-y",
          "@upstash/context7-mcp@latest"
        ]
      },
      "desktop-commander": {
        "command": "npx",
        "args": [
          "-y",
          "@wonderwhy-er/desktop-commander"
        ]
      },
      "mcp-deepwiki": {
        "command": "npx",
        "args": [
          "-y",
          "mcp-deepwiki@latest"
        ]
      },
      "chrome-mcp-server": {
        "command": "npx",
        "args": [
          "-y",
          "mcp-chrome-bridge"
        ],
        "env": {
          "NODE_ENV": "production"
        }
      },
      "Codebase MCP": {
        "command": "codebase-mcp",
        "args": [
          "start"
        ]
      },
      "ast-aware-editor": {
        "command": "npx",
        "args": [
          "-y",
          "ast-aware-editor"
        ],
        "env": {
          "NODE_ENV": "production",
          "AST_SERVER_PATH": "${AST_SERVER_PATH:-./ast-mcp-server}"
        }
      }
    }
  }